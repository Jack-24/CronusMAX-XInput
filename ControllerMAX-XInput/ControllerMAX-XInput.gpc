void main() {

    if(get_val(XB1_LX) < 0)
    {
        set_val(XB1_LX, ((get_val(XB1_LX) * 7) / 9) - 23);
    }
    if(get_val(XB1_LX) > 0)
    {
        set_val(XB1_LX, ((get_val(XB1_LX) * 7) / 9) + 23);
    }
    
}
